{
  "rules": {
    "max-len": [
      2,
      120,
      4
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "prefer-template": 2,
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 1,
    "object-shorthand": 0,
    "no-param-reassign": 1,
    "arrow-body-style": 0,
    "prefer-const": 1,
    "quotes": 1,
    "wrap-iife": [2, "inside"],
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-magic-numbers": 0,
    "func-names": 0,
    "angular/no-digest": 0,
    "angular/controller-as-vm": 0,
    "angular/rest-service": 0,
    "angular/foreach": 0,
    "angular/no-service-method": 0,
    "angular/window-service": 0,
    "angular/controller-name": 0,
    "comma-dangle": 0,
    "strict": 0,
    "no-else-return": 0,
    "no-undef": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "protractor": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint"
}
